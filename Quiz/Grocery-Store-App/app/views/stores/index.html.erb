<h1>Stores</h1>

  <table class="table table-dark">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Description</th>
        <th scope="col">Url</th>
        <th scope="col">Status</th>
        <th scope="col">Code</th>
        <th scope="col">Address</th>
        <th scope="col">City</th>
        <th scope="col">State</th>
        <th scope="col">Zip</th>
        <th scope="col">Country</th>
      </tr>
    </thead>
    <tbody>
      <% @stores.each do |store| %>
        <tr>
          <td><%= store.name %></td>
          <td><%= store.description %></td>
          <td><%= store.url %></td>
          <td><%= store.status %></td>
          <td><%= store.code %></td>
          <td><%= store.address %></td>
          <td><%= store.city %></td>
          <td><%= store.state %></td>
          <td><%= store.zip %></td>
          <td><%= store.country %></td>
          <td><button type="button" class="btn btn-outline-primary"><%= link_to 'Show', store %></button></td>
          <td><button type="button" class="btn btn-outline-primary"><%= link_to 'Edit', edit_employee_path(store) %></button></td>
          <td><button type="button" class="btn btn-outline-primary"><%= link_to 'Destroy', store, method: :delete, data: { confirm: 'Are you sure?' } %></button></td>
        </tr>
      <% end %>
    </tbody>
  </table>

<br>

<button type="button" class="btn btn-outline-primary"><%= link_to 'New Store', new_store_path %></button>

