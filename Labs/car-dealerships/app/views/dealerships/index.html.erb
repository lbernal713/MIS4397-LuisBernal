      <h1>Dealerships</h1>
        <div class="dealership-table">
        <table>
            <thead>
              <tr>
                <th>Name</th>
                <th>Address</th>
                <th>City</th>
                <th>State</th>
                <th>Zip</th>
                <th>Owner</th>
                <th colspan="3"></th>
              </tr>
            </thead>  
          <tbody>
            <% @dealerships.each do |dealership| %>
              <tr>
                <td>
                  <div class="card">
                    <div class="card-body">
                     <%= dealership.name %>
                     </div>
                  </div>
                </td>
                <td>
                  <div class="card">
                    <div class="card-body">
                     <%= dealership.address %>
                     </div>
                  </div>
                </td>
                <td>
                  <div class="card">
                    <div class="card-body">
                     <%= dealership.city %>
                     </div>
                  </div>
                </td>
                <td>
                  <div class="card">
                    <div class="card-body">
                     <%= dealership.state %>
                     </div>
                  </div>
                </td>
                <td>
                  <div class="card">
                    <div class="card-body">
                     <%= dealership.zip %>
                     </div>
                  </div>
                </td>
                <td>
                  <div class="card">
                    <div class="card-body">
                     <%= dealership.owner %>
                     </div>
                  </div>
                </td>
                <td>
                  <div class="card">
                    <div class="card-body">
                     <%= link_to 'Show', dealership %>
                     </div>
                  </div>
                </td>
                <td>
                  <div class="card">
                    <div class="card-body">
                     <%= link_to 'Edit', edit_dealership_path(dealership) %>
                     </div>
                  </div>
                </td>
                <td>
                  <div class="card">
                    <div class="card-body">
                     <%= link_to 'Destroy', dealership, method: :delete, data: { confirm: 'Are you sure?' } %>
                     </div>
                  </div>
                </td>
              </tr>
              
            <% end %>
          </tbody>
        </table>
        </div>
    <br>
    
    <%= link_to 'New Dealership', new_dealership_path %>
</div>